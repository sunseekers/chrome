{
  "name": "sunseekers 的都一个谷歌插件",
  "version": "0.1",
  "description": "windows:按Alt+S开启、关闭我的网站  \nmac:按Control+S开启、关闭sunseekers网站",
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "apple-touch-icon.png",
    "32": "apple-touch-icon.png",
    "48": "apple-touch-icon.png",
    "128": "apple-touch-icon.png"
  },
  "browser_action": {
    "default_icon": "apple-touch-icon.png",
    "default_popup": "sunseekers.html"
  },
  "commands": {
    "toggle-tags": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "MacCtrl+S"
      },
      "description": "Toggle Tags"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
